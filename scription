-- Scripts:

local function XWNFDV_fake_script() -- Main.Backdoor 
	local script = Instance.new('LocalScript', Main)

	local rep = game:GetService("ReplicatedStorage")
	local  table = {"remoteevent","RemoteEvent", "backdoor", "exploit", "admin", "re", "cmd", "script"}
	local found = false
	local name = nil
	
	local function Backdoors()
		for _,v in pairs(rep:GetDescendants()) do
			if v:IsA("RemoteEvent") then
				return {
					true,
					v.Name
				}
			end
		end
		
		return false
	end
	
	local function nameissuspection(names)
		if table.find(table,name) then
			print("[Backdoor]:Suspect name remote")
			return names
		end
	end
	
	script.Parent.SCAN.MouseButton1Click:Connect(function()
		local back = Backdoors()
		if back[1] then
			name = nameissuspection(back[2])
			script.Parent.Status.Text = "Found backdoors called "..back[2]
			found = true
			task.wait(2)
			script.Parent.Status.Text = "None"
		end
	end)
	
	script.Parent.execute.MouseButton1Click:Connect(function()
		if found then
			local remote = rep:WaitForChild(name)
			remote:FireServer(script.Parent.TextBox.Text)
		else
			script.Parent.Status.Text = "need to scan first!"
		end
	end)
end
coroutine.wrap(XWNFDV_fake_script)()
local function SBMXU_fake_script() -- Main.LocalScript 
	local script = Instance.new('LocalScript', Main)

	local UIS = game:GetService("UserInputService")
	local frame = script.Parent
	
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale,startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService("TweenService"):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(SBMXU_fake_script)()
